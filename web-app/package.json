		"@sveltejs/kit": "^2.20.7",
		"@tailwindcss/forms": "^0.5.3",
		"@typescript-eslint/eslint-plugin": "^8.31.1",
		"@typescript-eslint/parser": "^8.31.1",
		"autoprefixer": "^10.4.7",
		"eslint": "^9.25.1",
		"eslint-config-prettier": "^10.1.2",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.31",
